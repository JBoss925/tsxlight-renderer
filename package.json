{
  "name": "tsx-lightweight-renderer",
  "version": "1.0.0",
  "description": "A lightweight tsx renderer without the ",
  "main": "./index.js",
  "scripts": {
    "start": "ts-node ./src",
    "start:watch": "nodemon",
    "electronStart": "electron .",
    "compile": "tsc --jsxFactory tsxlight.createElement --esModuleInterop --allowUmdGlobalAccess --jsx react",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.9.7",
    "@types/electron": "^1.6.10",
    "@types/express": "^4.17.3",
    "@types/express-ws": "^3.0.0",
    "@types/jsdom": "^16.1.0",
    "@types/node": "^13.9.2",
    "@types/react": "^16.9.23",
    "@types/socket.io": "^2.1.4",
    "@types/uuid": "^7.0.0",
    "@types/websocket": "^1.0.0",
    "electron": "^8.2.4",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "firebase-admin": "^8.10.0",
    "jsdom": "^16.2.1",
    "material-ui-chip-input": "^2.0.0-beta.2",
    "nodemon": "^2.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "socket.io": "^2.3.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3",
    "uuid": "^7.0.2",
    "websocket": "^1.0.31"
  },
  "nodemonConfig": {
    "ignore": [
      ".git",
      "node_modules"
    ],
    "watch": [
      "./src"
    ],
    "exec": "npm start",
    "ext": "tsx"
  }
}